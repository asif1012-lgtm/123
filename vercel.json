{
  "version": 2,
  "buildCommand": "cd client && vite build",
  "outputDirectory": "client/dist",
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg|webmanifest))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}